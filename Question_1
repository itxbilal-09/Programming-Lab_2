# Input
countries = {
    "France": "Paris",
    "Japan": "Tokyo",
    "Brazil": "Bras√≠lia",
    "Egypt": "Cairo",
    "Australia": "Canberra"
}
print("Countries:")
key = list(countries.keys())
i = 0
while i < len(key):
    print(key[i])
    i += 1

